pipeline {
    agent any

    stages {
        stage('Exportiong from OCI') {
            steps {
                git 'https://github.com/Achrafkhaterchi/OCI_2_AZURE.git'

                sh 'python3 Export_Identity.py OCI.xlsx terraform.tfstate'

                archiveArtifacts 'terraform.tfstate'
            }
        }
    }

    post {
        success {
            echo 'Success !'
        }
    }
}
